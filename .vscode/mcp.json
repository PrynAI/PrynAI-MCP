{
    "inputs": [
        {
            "id": "token",
            "type": "promptString",
            "title": "Paste an access token",
            "description": "Get a token via MSAL client credentials",
            "password": true
        }
    ],
    "servers": {
        "PrynAI MCP": {
            "type": "http",
            "url": "https://mcp.prynai.com/mcp",
            "headers": {
                "Authorization": "Bearer ${input:token}"
            }
        }
    }
}